<section>
<div class="container">
  <div class="card" style="width:400px">
   <form method="post">
      <div class="card-body">
      <h4 class="card-title"></h4>
      <p class="card-text"> <%= medicinlist[i].id %></p>
      <p class="card-text"> <%= medicinlist[i].name %></p>
      <p class="card-text"> <%= medicinlist[i].vendorName %></p>
      <p class="card-text"> <%= medicinlist[i].price %></p>
      <p class="card-text"> <%= medicinlist[i].segmentation %></p>
      <p class="card-text"> <%= medicinlist[i].genre %></p>
      <p class="card-text"> <%= medicinlist[i].amount %></p>
      <p class="card-text"> <%= medicinlist[i].availability %></p>
      </div>
          <input class="btn btn-info" type="submit" name="submit">
       </div>
    </form>
  <br>    
</section>
